{
  "name": "LetsLearnReactNative",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "pod": "pod install --project-directory=ios",
    "pod-repo-update": "pod install --project-directory=ios --repo-update",
    "android": "react-native run-android",
